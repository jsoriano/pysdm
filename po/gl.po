#: ../pysdm/fsdata/constants.py:8
msgid "Mounting"
msgstr "Montado"

#: ../pysdm/fsdata/constants.py:8
msgid "Special Files"
msgstr "Ficheiros especiais"

#: ../pysdm/fsdata/constants.py:8
msgid "Journaling"
msgstr "«Journaling»"

#: ../pysdm/fsdata/constants.py:8
msgid "Quota"
msgstr "Cota"

#: ../pysdm/fsdata/constants.py:8
msgid "Performance"
msgstr "Rendimento"

#: ../pysdm/fsdata/constants.py:8
msgid "Miscellaneous"
msgstr "Outras opcións"

#: ../pysdm/fsdata/default.py:5
msgid "Update file time for each access"
msgstr "Actualizar o indicador de tempo en cada acceso"

#: ../pysdm/fsdata/default.py:6
msgid "Update directory time for each access"
msgstr "Actualizar o indicador de tempo de directorios en cada acceso"

#: ../pysdm/fsdata/default.py:7
msgid "The file system is mounted at boot time"
msgstr "Montar o sistema de ficheiros no arranque"

#: ../pysdm/fsdata/default.py:8
msgid "Interpret character or block special devices on the file system"
msgstr "Interpretar dispositivos especiais de carácteres ou bloques"

#: ../pysdm/fsdata/default.py:9
msgid "Permit execution of binaries"
msgstr "Permitir a execución de binarios"

#: ../pysdm/fsdata/default.py:10
msgid "Allow a user to mount and unmount the file system"
msgstr "Permitir a un usuario montar e desmontar o sistema de ficheiros"

#: ../pysdm/fsdata/default.py:11
msgid "Allow any user to mount the file system"
msgstr "Permitir a calquer usuario montar o sistema de ficheiros"

#: ../pysdm/fsdata/default.py:12
msgid "Allow a group to mount this file system"
msgstr "Permitir a un grupo montar o sistema de ficheiros"

#: ../pysdm/fsdata/default.py:13
msgid "This file system requires network"
msgstr "Este sistema de ficheiros require conexión de rede"

#: ../pysdm/fsdata/default.py:14
msgid "The owner of the device can mount it"
msgstr "O propietario do dispositivo pode montalo"

#: ../pysdm/fsdata/default.py:15
msgid "Permit executables to change user/group identity"
msgstr "Permitir aos executables cambiar o identificador de usuario/grupo"

#: ../pysdm/fsdata/default.py:16
msgid "Mount file system in read-only mode"
msgstr "Montar como sistema de só lectura"

#: ../pysdm/fsdata/default.py:17
msgid "I/O to the file system should be done synchronously"
msgstr "E/S no sistema de ficheiros debe facerse sincronizadamente"

#: ../pysdm/fsdata/default.py:18
msgid "All directory updates should be done synchronously"
msgstr "Todas as actualizacións de directorios deben facerse sincronizadamente"

#: ../pysdm/fsdata/ext2.py:5
#: ../pysdm/fsdata/ext3.py:5
msgid "Support POSIX access list"
msgstr "Soporte para listas de acceso POSIX"

#: ../pysdm/fsdata/ext2.py:6
#: ../pysdm/fsdata/ext3.py:6
msgid "Minix behaviour for stat information"
msgstr "Comportamento «Minix» para información de estado"

#: ../pysdm/fsdata/ext2.py:7
#: ../pysdm/fsdata/ext3.py:7
msgid "Check file system at mount time"
msgstr "Comprobar o sistema de ficheiros ao montar"

#: ../pysdm/fsdata/ext2.py:8
#: ../pysdm/fsdata/ext3.py:8
msgid "Print debugging info"
msgstr "Amosar a información de depuración"

#: ../pysdm/fsdata/ext2.py:9
#: ../pysdm/fsdata/ext3.py:9
msgid "Specify what to do in case of error"
msgstr "Especificar que facer en caso de erro"

#: ../pysdm/fsdata/ext2.py:10
#: ../pysdm/fsdata/ext3.py:10
msgid "Get group id from the directory for a new file"
msgstr "Aplicar aos novos ficheiros o identificador de grupo do directorio"

#: ../pysdm/fsdata/ext2.py:11
#: ../pysdm/fsdata/ext3.py:11
msgid "Do not attach buffer_heads to file pagecache"
msgstr "Non adxuntar cabeceiras do búfer ao ficheiro de caché"

#: ../pysdm/fsdata/ext2.py:12
#: ../pysdm/fsdata/ext3.py:12
msgid "Disables 32 bit user and group id"
msgstr "Desactivar os identificadores de grupo e usuario de 32 bits"

#: ../pysdm/fsdata/ext2.py:13
#: ../pysdm/fsdata/ext3.py:13
msgid "Use old new inode allocator"
msgstr "Usar o anterior localizador de espazo para novos nodos-i"

#: ../pysdm/fsdata/ext2.py:14
#: ../pysdm/fsdata/ext3.py:14
msgid "Group allowed to use reserved space"
msgstr "Grupo que pode usar o espazo reservado"

#: ../pysdm/fsdata/ext2.py:15
#: ../pysdm/fsdata/ext3.py:15
msgid "User allowed to use reserved space"
msgstr "Usuario que pode usar o espazo reservado"

#: ../pysdm/fsdata/ext2.py:16
#: ../pysdm/fsdata/ext3.py:16
msgid "Use a especific block as superblock"
msgstr "Usar o bloque específico como superbloque"

#: ../pysdm/fsdata/ext2.py:17
#: ../pysdm/fsdata/ext3.py:17
msgid "Support extended attributes"
msgstr "Compatibilidade cos atributos estendidos"

#: ../pysdm/fsdata/ext3.py:18
msgid "Update journal or specify the inode where it is"
msgstr "Actualizar «journal» ou especificar o nodo-i donde está"

#: ../pysdm/fsdata/ext3.py:19
msgid "Do not load journal on mounting"
msgstr "Non cargar «journal» ao montar"

#: ../pysdm/fsdata/ext3.py:20
msgid "Specify a journaling mode"
msgstr "Especificar un modo de «journaling»"

#: ../pysdm/fsdata/ext3.py:21
msgid "Synchronize data and metadata every specified seconds, 0 for default (5s.)"
msgstr "Especificar o tempo de actualización de datos e metadatos (0 para valor predeterminado)"

#: ../pysdm/fsdata/fat.py:5
msgid "Set blocksize"
msgstr "Establecer o tamaño do bloque"

#: ../pysdm/fsdata/fat.py:6
#: ../pysdm/fsdata/ntfs.py:9
msgid "Owner user of the filesystem"
msgstr "Usuario propietario do sistema de ficheiros"

#: ../pysdm/fsdata/fat.py:7
#: ../pysdm/fsdata/ntfs.py:10
msgid "Owner group of the filesystem"
msgstr "Grupo propietario do sistema de ficheiros"

#: ../pysdm/fsdata/fat.py:8
msgid "umask for file and directory permissions in octal"
msgstr "«umask» para os permisos de ficheiros e directorios (en octal)"

#: ../pysdm/fsdata/fat.py:9
msgid "umask for directory permissions in octal"
msgstr "«umask» para os permisos de directorios (en octal)"

#: ../pysdm/fsdata/fat.py:10
#: ../pysdm/fsdata/ntfs.py:11
msgid "umask for file permissions in octal"
msgstr "«umask» para os permisos de ficheiros (en octal)"

#: ../pysdm/fsdata/fat.py:11
msgid "Select level of character checking in file names"
msgstr "Seleccionar o nivel de comprobación de carácteres nos nomes de ficheiros"

#: ../pysdm/fsdata/fat.py:12
msgid "Sets the codepage for converting to shortname characters"
msgstr "Establecer a colección de carácteres para converter carácteres"

#: ../pysdm/fsdata/fat.py:13
msgid "Text format conversion mode"
msgstr "Modo de conversión de formato de texto"

#: ../pysdm/fsdata/fat.py:14
msgid "Forces the driver to use the CVF module instead of autodetection"
msgstr "Forzar ao controlador a usar o módulo CVF no canto de autodetectalo"

#: ../pysdm/fsdata/fat.py:15
msgid "Option passed to CVF module"
msgstr "Opción pasada ao módulo CVF"

#: ../pysdm/fsdata/fat.py:16
msgid "Display debug messages"
msgstr "Amosar as mensaxes de depuración"

#: ../pysdm/fsdata/fat.py:17
msgid "Specify a 12, 16 or 32 bit fat"
msgstr "Especificar unha FAT de 12, 16 o 32 bits"

#: ../pysdm/fsdata/fat.py:18
#: ../pysdm/fsdata/ntfs.py:5
msgid "Character set to use for file names"
msgstr "Colección de carácteres para os nomes de ficheiro"

#: ../pysdm/fsdata/fat.py:19
msgid "Failures do not return errors"
msgstr "Os fallos no devolven erros"

#: ../pysdm/fsdata/ntfs.py:6
msgid "Use UTF-8 for converting file names"
msgstr "Usar UTF-8 para converter nomes de ficheiros"

#: ../pysdm/fsdata/ntfs.py:7
msgid "Use escape sequences for unknown characters"
msgstr "Usar secuencias de escape para carácteres descoñecidos"

#: ../pysdm/fsdata/ntfs.py:8
msgid "If enabled (1), the file system distinguishes between upper and lower case."
msgstr "Se se activa (1), o sistema de ficheiros distingue entre maiúsculas e minúsculas"

#: ../pysdm/fsdata/reiserfs.py:5
msgid "Updated filesystem from ReiserFS 3.5 to 3.6"
msgstr "Sistema de ficheiros actualizado de ReiserFS 3.5 a 3.6"

#: ../pysdm/fsdata/reiserfs.py:6
msgid "Specify hash function to find files"
msgstr "Especificar a función «hash» para atopar ficheiros"

#: ../pysdm/fsdata/reiserfs.py:7
msgid "Disable reiserfs journaling"
msgstr "Desactivar «journaling»"

#: ../pysdm/fsdata/reiserfs.py:8
msgid "Disable efficient in-tree storage of small files"
msgstr "Desactivar o empaquetado de ficheiros na árbore"

#: ../pysdm/fsdata/swap.py:5
msgid "Swap device"
msgstr "Dispositivo de intercambio"

#: ../pysdm/fsdata/xfs.py:5
msgid "Enable the DMAPI (Data management API)"
msgstr "Activar DMAPI (API de xestión de datos)"

#: ../pysdm/fsdata/xfs.py:6
msgid "Data allocations will not be aligned at stripe unit boundaries"
msgstr "A ubicación dos datos non será aliñada nos límites das unidades segmentadas"

#: ../pysdm/fsdata/xfs.py:7
msgid "The file system will be mounted without running log recovery"
msgstr "O sistema de ficheiros será montado sen rexistro de recuperación"

#: ../pysdm/fsdata/xfs.py:8
msgid "Ignore file system uuid"
msgstr "Ignorar UUID"

#: ../pysdm/fsdata/xfs.py:9
msgid "Enable user disk quota"
msgstr "Activar cota de usuario"

#: ../pysdm/fsdata/xfs.py:10
msgid "Enable group disk quota"
msgstr "Activar cota de grupo"

