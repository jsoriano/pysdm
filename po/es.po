msgid "Storage Device Manager"
msgstr "Gestor de dispositivos de almacenamiento"

#: ../data/pysdm.glade.h:8
msgid "Type"
msgstr "Tipo"

#: ../data/pysdm.glade.h:9
msgid "Mountpoint"
msgstr "Punto de Montaje"

#: ../data/pysdm.glade.h:10
msgid "Name"
msgstr "Nombre"

#: ../data/pysdm.glade.h:11
msgid "Options"
msgstr "Opciones"

#: ../data/pysdm.glade.h:12
msgid "Set defaults"
msgstr "Por defecto"

#: ../data/pysdm.glade.h:13
msgid "Assistant"
msgstr "Asistente"

#: ../data/pysdm.glade.h:14
msgid "Mount"
msgstr "Montar"

#: ../data/pysdm.glade.h:15
msgid "Information"
msgstr "Información"

#: ../data/pysdm.glade.h:16
msgid "General Configuration"
msgstr "Configuración General"

#: ../pysdm/fsdata/default.py:3
msgid "Update file time for each access"
msgstr "Actualizar indicador de tiempo en cada acceso"

#: ../pysdm/fsdata/default.py:4
msgid "The filesystem is mounted in boot time"
msgstr "Montar el sistema de ficheros en el arranque"

#: ../pysdm/fsdata/default.py:5
msgid "Interpret character or block special devices on the file system"
msgstr "Interpretar dispositivos de carácteres o bloques"

#: ../pysdm/fsdata/default.py:6
msgid "Allow execution of binaries"
msgstr "Permitir ejecución de binarios"

#: ../pysdm/fsdata/default.py:7
msgid "Allows a user to mount and unmount the filesystem"
msgstr "Permitir a un usuario montar y desmontar el sistema de archivos"

#: ../pysdm/fsdata/default.py:8
msgid "Allows any user to mount the filesystem"
msgstr "Permitir a cualquier usuario montar el sistema de archivos"

#: ../pysdm/fsdata/default.py:9
msgid "Allow a group to mount this filesystem"
msgstr "Permitir a un grupo montar el sistema de archivos"

#: ../pysdm/fsdata/default.py:10
msgid "This file system requires network"
msgstr "Este sistema de archivos requiere conexión"

#: ../pysdm/fsdata/default.py:11
msgid "The owner of the device can mount it"
msgstr "El propietario del dispositivo puede montarlo"

#: ../pysdm/fsdata/default.py:12
msgid "Allow set user identifier"
msgstr "Permitir establecer el identificador de usuario (suid)"

#: ../pysdm/fsdata/default.py:13
msgid "Mount file system in read-only mode"
msgstr "Sistema de solo lectura"

#: ../pysdm/fsdata/default.py:14
msgid "I/O to the file system should be done synchronously"
msgstr "E/S debe hacerse sincrinizadamente"

#: ../pysdm/fsdata/default.py:15
msgid "All directory updates should be done synchronously"
msgstr ""
"Todas las actualizaciones de directorios deben hacerse sincronizadamente"

#: ../pysdm/fsdata/ext2.py:3 ../pysdm/fsdata/ext3.py:3
msgid "Support POSIX access list"
msgstr "Soporte para listas de acceso POSIX"

#: ../pysdm/fsdata/ext2.py:4 ../pysdm/fsdata/ext3.py:4
msgid "Minix behaviour for stat information"
msgstr "Comportamiento Minix para información de estado"

#: ../pysdm/fsdata/ext2.py:5 ../pysdm/fsdata/ext3.py:5
msgid "Check filesystem at mount time"
msgstr "Comrpobar sistema de archivos en el momento de mon"

#: ../pysdm/fsdata/ext2.py:6 ../pysdm/fsdata/ext3.py:6
msgid "Print debugging info"
msgstr "Mostrar información de debug"

#: ../pysdm/fsdata/ext2.py:7 ../pysdm/fsdata/ext3.py:7
msgid "Specify what to do in case of error"
msgstr "Especificar que hacer en caso de error"

#: ../pysdm/fsdata/ext2.py:8 ../pysdm/fsdata/ext3.py:8
msgid "Get group id from the directory for a new file"
msgstr "Aplicar a los nuevos archivos el identificador de grupo del directorio"

#: ../pysdm/fsdata/ext2.py:9 ../pysdm/fsdata/ext3.py:9
msgid "Do not attach buffer_heads to file pagecache"
msgstr "No adjuntar cabeceras de buffer al archivo de cache"

#: ../pysdm/fsdata/ext2.py:10 ../pysdm/fsdata/ext3.py:10
msgid "Disables 32 bit user and group id"
msgstr "Desactivar identificadores de grupo y usuario de 32 bits"

#: ../pysdm/fsdata/ext2.py:11 ../pysdm/fsdata/ext3.py:11
msgid "Use old new inode allocator"
msgstr "Usar anterior localizador de espacio para nuevos inodos"

#: ../pysdm/fsdata/ext2.py:12 ../pysdm/fsdata/ext3.py:12
msgid "Group allowed to use reserved space"
msgstr "Grupo que puede usar el espacio reservado"

#: ../pysdm/fsdata/ext2.py:13 ../pysdm/fsdata/ext3.py:13
msgid "User allowed to use reserved space"
msgstr "Usuario que puede usar el espacio reservado"

#: ../pysdm/fsdata/ext2.py:14 ../pysdm/fsdata/ext3.py:14
msgid "Use a especific block as superblock"
msgstr "Especificar bloque como superbloque"

#: ../pysdm/fsdata/ext2.py:15 ../pysdm/fsdata/ext3.py:15
msgid "Support extended attributes"
msgstr "Soportar atributos extendidos"

#: ../pysdm/fsdata/ext3.py:16
msgid "Update journal or especify the inode where it is"
msgstr "Actualizar journal o especificar el inodo donde está"

#: ../pysdm/fsdata/ext3.py:17
msgid "Do not load journal on mounting"
msgstr "No cargar journal al montar"

#: ../pysdm/fsdata/ext3.py:18
msgid "Especify a journaling mode"
msgstr "Especificar un modo de journaling"

#: ../pysdm/fsdata/ext3.py:19
msgid ""
"Synchronize data and metadata every specified seconds, 0 for default (5s.)"
msgstr ""
"Especificar tiempo de actualización de datos y metadatos (0 para valor por "
"defecto)"

#: ../pysdm/fsdata/fat.py:3
msgid "Set blocksize"
msgstr "Establecer el tamaño de bloque"

#: ../pysdm/fsdata/fat.py:4 ../pysdm/fsdata/ntfs.py:7
msgid "Owner user of the filesystem"
msgstr "Usuario propietario del sistema de ficheros"

#: ../pysdm/fsdata/fat.py:5 ../pysdm/fsdata/ntfs.py:8
msgid "Owner group of the filesystem"
msgstr "Usuario propietario del sistema de ficheros"

#: ../pysdm/fsdata/fat.py:6
msgid "umask for file and directory permissions in octal"
msgstr "umask para los permisos de archivos y directorios (en octal)"

#: ../pysdm/fsdata/fat.py:7
msgid "umask for directory permissions in octal"
msgstr "umask para los permisos de directorios (en octal)"

#: ../pysdm/fsdata/fat.py:8 ../pysdm/fsdata/ntfs.py:9
msgid "umask for file permissions in octal"
msgstr "umask para los permisos de archivos (en octal)"

#: ../pysdm/fsdata/fat.py:9
msgid "Select level of character checking in file names"
msgstr ""
"Seleccionar nivel de comprobación de carácteres en los nombres de archivos"

#: ../pysdm/fsdata/fat.py:10
msgid "Sets the codepage for converting to shortname characters"
msgstr "Establecer colección de carácteres para convertir carácteres"

#: ../pysdm/fsdata/fat.py:11
msgid "Text format conversion mode"
msgstr "Modo de conversión de formato de texto"

#: ../pysdm/fsdata/fat.py:12
msgid "Forces the driver to use the CVF module instead of autodetection"
msgstr "Forzar al controlador a usar el módulo CVF en lugar de autodetectarlo"

#: ../pysdm/fsdata/fat.py:13
msgid "Option passed to CVF module"
msgstr "Opción pasada al módulo CVF"

#: ../pysdm/fsdata/fat.py:14
msgid "Display debug messages"
msgstr "Mostrar mensajes de debug"

#: ../pysdm/fsdata/fat.py:15
msgid "Specify a 12, 16 or 32 bit fat"
msgstr "Especificar una FAT de 12, 16 o 32 bits"

#: ../pysdm/fsdata/fat.py:16 ../pysdm/fsdata/ntfs.py:3
msgid "Character set to use for file names"
msgstr "Colección de carácteres para los nombres de archivo"

#: ../pysdm/fsdata/fat.py:17
msgid "Failures do not return errors"
msgstr "Los fallos no devuelven errores"

#: ../pysdm/fsdata/ntfs.py:4
msgid "Use UTF-8 for converting file names"
msgstr "Usar UTF-8 para convertir nombres de archivos"

#: ../pysdm/fsdata/ntfs.py:5
msgid "Use escape sequences for unknown characters"
msgstr "Usar secuencias de escape para carácteres desconocidos"

#: ../pysdm/fsdata/ntfs.py:6
msgid ""
"If enabled (1), the file system distinguishes between upper and lower case."
msgstr ""
"Si se activa (1), el sistema de ficheros distingue entre mayúsculas y "
"minúsculas"

#: ../pysdm/fsdata/reiserfs.py:3
msgid "Updated filesystem from ReiserFS 3.5 to 3.6"
msgstr "Sistema de ficheros actualizado de ReiserFS 3.5 a 3.6"

#: ../pysdm/fsdata/reiserfs.py:4
msgid "Especify hash function to find files"
msgstr "Especificar función hash para encontrar archivos"

#: ../pysdm/fsdata/reiserfs.py:5
msgid "Disable reiserfs journaling"
msgstr "Desactivar journaling"

#: ../pysdm/fsdata/reiserfs.py:6
msgid "Disable packing of files into the tree"
msgstr "Desactivar empaquetado de archivos en el árbol"

#: ../pysdm/fsdata/swap.py:3
msgid "Swap device"
msgstr "Dispositivo swap"

#: ../pysdm/fsdata/xfs.py:3
msgid "Enable the DMAPI (Data management API)"
msgstr "Activar DMAPI (API de gestión de datos)"

#: ../pysdm/fsdata/xfs.py:4
msgid "Data allocations will not be aligned at stripe unit boundaries"
msgstr ""

#: ../pysdm/fsdata/xfs.py:5
msgid "The filesystem will be mounted without running log recovery"
msgstr "El sistema de ficheros será montado sin log de recuperación"

#: ../pysdm/fsdata/xfs.py:6
msgid "Ignore file system uuid"
msgstr "Ignorar uuid"

#: ../pysdm/fsdata/xfs.py:7
msgid "Enable user disk quota"
msgstr "Activar cuota de usuario"

#: ../pysdm/fsdata/xfs.py:8
msgid "User disk quota limits enforced"
msgstr "Limites de cuota de usuario forzados"

#: ../pysdm/fsdata/xfs.py:9
msgid "Enable group disk quota"
msgstr "Activar cuota de grupo"

#: ../pysdm/fsdata/xfs.py:10
msgid "Group disk quota limits enforced"
msgstr "Limites de cuota de grupo forzados"

#: ../pysdm/pysdm.py:55
msgid "Partition List"
msgstr "Particiones"

#: ../pysdm/pysdm.py:182
msgid "Configure now?"
msgstr "¿Configurar ahora?"

#: ../pysdm/pysdm.py:183
msgid ""
" hasn't been configured.\n"
"Do you want to configure it now?\n"
msgstr ""
"·no ha sido configurada.\n"
"¿Configurar ahora?\n"

#: ../pysdm/pysdm.py:212
msgid "Select a file for mountpoint..."
msgstr "Elige un directorio como punto de montaje..."

#: ../pysdm/pysdm.py:241
msgid "Select options"
msgstr "Elige opciones"

#: ../pysdm/pysdm.py:260
msgid ""
"The file system is busy.\n"
"Can't be remounted with the new location"
msgstr ""
"El sistema de ficheros está ocupado.\n"
"No puede ser remontado en la nueva ubicación."

#: ../pysdm/pysdm.py:262
msgid ""
"The file system can't be mounted\n"
"mount returned error:"
msgstr "El sistema de ficheros no pudo ser montado\n"
"mount devolvió el error:"

#: ../pysdm/pysdm.py:264
msgid "Mount error"
msgstr "Error de montaje"

